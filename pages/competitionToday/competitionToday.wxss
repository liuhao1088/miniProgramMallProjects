.header_maxdiv {
  width: 100%;
  height: 350rpx;
  background-color: #FF4444;
  border-radius: 60%/0 0 25% 25%
}

.tab_div {
  width: 100%;
  height: 100rpx;
  border: 0rpx solid black;
  text-align: center;
}

.tab {
  width: calc(98%/2);
  height: 100%;
  border: 0rpx solid gold;
  color: white;
  float: left;
}

.tab_nr1 {
  width: 100%;
  height: 60%;
  line-height: 60rpx;
  font-size: 40rpx;
  font-weight: 700;
  float: left;
}

.tab_nr2 {
  width: 100%;
  height: 30%;
  line-height: 30rpx;
  font-size: 30rpx;
  font-weight: 600;
  float: left;
}

.tab_nr3 {
  width: 100%;
  height: 60%;
  line-height: 60rpx;
  font-size: 40rpx;
  float: left;
  font-weight: 300;
}

.tab_nr4 {
  width: 100%;
  height: 30%;
  line-height: 30rpx;
  font-size: 30rpx;
  float: left;
  font-weight: 300;
}

.content {
  width: 95%;
  margin: auto;
  border: 0rpx solid black;
  margin-top: 20rpx;
}

.czt_div {
  width: 100%;
  height: 420rpx;
  border: 0rpx solid blue;
  background-color: white;
  border-radius: 10rpx;
}

.main_div {
  width: 100%;
  height: 60rpx;
  border: 0rpx solid red;
  margin-top: 30rpx;
}

.main_div_title {
  height: 100%;
  line-height: 60rpx;
  font-size: 30rpx;
  font-weight: 700;
  float: left;
  margin-left: 20rpx;
}

.main_div_down {
  height: 100%;
  margin-left: 10rpx;
  float: left;
}

.item {
  display: inline-block;
  width: 18px;
  height: 30rpx;
  margin-right: 5px;
  color: #fff;
  font-size: 12px;
  text-align: center;
  background-color: #FF4444;
  border-radius: 2px;
  margin-top: 15rpx;
  line-height: 30rpx;
}

.point {
  margin-right: 5px;
  color: #FF4444;
}

.products_div {
  width: 100%;
  height: 270rpx;
  margin-top: 20rpx;
  border: 0rpx solid black;
}

.products_maxdiv {
  height: 100%;
  width: calc(100%/3);
  border: 0rpx solid red;
  display: flex;
  justify-content: center;
  align-items: center;
  float: left;
}

.products {
  height: 100%;
  width: 86%;
  border: 0rpx solid black;
}

.products_img {
  height: 70%;
  width: 100%;
  border-radius: 10rpx;
  background-color: #42AAE4;
  float: left;
}

.products_description {
  width: 100%;
  font-size: 24rpx;
  font-weight: 600;
  line-height: 50rpx;
  float: left;
}

.nrdiv_tagdiv {
  height: 20%;
  width: 100%;
  float: left;
  border: 0rpx solid blue;
  display: flex;
  /* justify-content: center; */
  align-items: center;
}

.nrdiv_maxdiv {
  width: 100%;
  margin-left: 15%;
}

.tag {
  float: left;
  position: relative;
  bottom: -4rpx;
  border: 0rpx solid gold;
  font-weight: 600;
  text-align: center;
  line-height: 15rpx;
  color: #FF4444;
  font-size: 28rpx;
}

.num {
  float: left;
  font-weight: 700;
  text-align: center;
  line-height: 15rpx;
  color: #FF4444;
  font-size: 40rpx;
}

.title_div {
  width: 100%;
  border: 0rpx solid black;
  margin-top: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  float: left;
}

.title {
  line-height: 50rpx;
  font-size: 40rpx;
  font-weight: 700;
  float: left;
}

.description {
  float: left;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #D2D3D4;
  margin-left: 20rpx;
}

.rmt_products_div {
  width: 100%;
  border-radius: 10rpx;
  background-color: white;
  float: left;
}

.rmt_div {
  width: 100%;
  height: 200rpx;
  border: 0rpx solid red;
  float: left;
  padding-bottom: 30rpx;
}

.rmt_img_div {
  height: 100%;
  width: 38%;
  border: 0rpx solid black;
  display: flex;
  justify-content: center;
  align-items: center;
  float: left;
}

.rmt_img {
  height: 100%;
  width: 90%;
  background-color: #FADDDF;
}

.rmt_nr_div {
  height: 100%;
  width: 60%;
  float: left;
  border: 0rpx solid blueviolet;
}

.rmt_nr {
  width: 100%;
  line-height: 36rpx;
  font-size: 28rpx;
  font-weight: 700;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.progress_maxdiv {
  width: 100%;
  height: 40rpx;
  float: left;
  border: 0rpx solid black;
}

.progress {
  width: 100%;
}

view .van-progress__pivot {
  right: unset;
}

view .van-progress {
  top: 22rpx;
}

.rmt_jg_div {
  width: 100%;
  height: 60rpx;
  float: left;
  border: 0rpx solid red;
  margin-top: 24rpx;
}

