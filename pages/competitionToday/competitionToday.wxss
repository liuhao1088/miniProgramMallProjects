}

.yj_div{
  height: 100%;
  line-height: 90rpx;
  font-size: 30rpx;
  float: left;
  margin-left: 20rpx;
  color: #D2D3D4;
  text-align: center;
  text-decoration: line-through;
}

.msq_div{
  float: right;
  height: 50rpx;
  width: 150rpx;
  border-radius: 40rpx;
  background-color: #FF4444;
  color: white;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  line-height: 50rpx;
  margin-top: 10rpx;
}


.column {
  height: 550rpx;
  width: 100%;
  border: 0rpx solid red;
  margin: auto;
  margin-top: 20rpx;
  float: left;
  padding-bottom: 20rpx;
}

.column_div {
  width: calc(98%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 10rpx;
  background-color: white;
  float: left;
}

.column_imgdiv {
  height: 65%;
  width: 100%;
  background-color: #283758;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  
}

.column_nrdiv {
  width: 95%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  margin: auto;
  padding-top: 15rpx;
}

.column_jgdiv{
  width: 95%;
  height: 10%;
  margin: auto;
  border: 0rpx solid red;
  margin-top: 30rpx;
}

.column_jg{
  height: 100%;
  width: 40%;
  float: left;
}

.column_xsdiv{
  width: 50%;
  height: 100%;
  float: right;
  text-align: center;
  line-height: 80rpx;
  color: #999999;
  font-size: 24rpx;
}

.mryg_maxdiv{
  width: 100%;
  float: left;
  background-color: white;
  border-radius: 10rpx;
  border: 0rpx solid blue;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.txw_div{
  float: right;
  height: 50rpx;
  width: 150rpx;
  border-radius: 40rpx;
  color: #FF4444;
  font-size: 30rpx;
  font-weight: 600;
  text-align: center;
  line-height: 50rpx;
  margin-top: 10rpx;
  border: 2rpx solid #FF4444;
}

.return{
  height: 60rpx;
  width: 60rpx;
  border: 2rpx solid #CACACA;
  position: fixed;
  z-index: 102;
  bottom: 28%;
  right: 40rpx;
  border-radius: 50%;
  background-color: #FEFEFE;
  text-align: center;
  line-height: 60rpx;
  color: #666666;
  display: block;
}

