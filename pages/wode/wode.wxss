  border-top-right-radius: 10rpx;
  
}

.column_nrdiv {
  width: 95%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 28rpx;
  margin: auto;
  padding-top: 15rpx;
}

.column_jgdiv{
  width: 95%;
  height: 10%;
