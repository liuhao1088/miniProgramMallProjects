}


.main {
  width: 95%;
  padding-bottom: 40rpx;
}

.column {
  height: 550rpx;
  width: 100%;
  border: 0rpx solid red;
  margin: auto;
  margin-top: 20rpx;

}

.column_div {
  width: calc(98%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 10rpx;
  background-color: white;
  float: left;
}

.column_imgdiv {
  height: 65%;
